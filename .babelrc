{
  "presets": [
    [ "@babel/env", {
      "targets": {
        "edge": "17",
        "firefox": "60",
        "chrome": "67",
        "safari": "11.1",
        "ie": "11"
      },
      "useBuiltIns": "usage",
      "corejs": { "version": "3.17", "proposals": true }
    } ]
  ],
  "plugins": [ "@babel/plugin-proposal-class-properties" ]
}